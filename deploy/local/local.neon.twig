parameters:
	default:
		host: "{{ MYSQL_HOST }}"
		dbname: "{{ MYSQL_DATABASE }}"
		user: "{{ MYSQL_USER }}"
		password: "{{ MYSQL_PASSWORD }}"
	production_mode: {{ PRODUCTION_MODE ? true : false }}
	cycle:
		host: "{{ POSTGRES_HOST }}"
		dbname: "{{ POSTGRES_DB }}"
		user: "{{ POSTGRES_USER }}"
		password: "{{ POSTGRES_PASSWORD }}"
		port: "{{ POSTGRES_PORT }}"
	redis:
		uri: {{ REDIS_DSN }}
		database: {{ REDIS_CYCLE_DB }}